<h1>Listing employees</h1>

<div>
<%@employees.each do |i|%>
<%if !i.employee_list.blank?%>
     <ul>     
     <li>
     <%= i.name%>
		 
		  <%i.employee_list.each do |k|%>
			<ul>
		 <li>
		   <%= k.name%>
		 </li>
		 </ul>
		 <%end%>
     </li>
		
     </ul>
     <%end%>
<%end%>
</div>

<br />

<%= link_to 'New Employee', new_employee_path %>
